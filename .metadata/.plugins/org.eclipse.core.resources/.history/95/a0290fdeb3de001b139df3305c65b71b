package com.in28minutes.learnspringframework.game;

public class MarioGame {
	
	public void up() {
		System.out.println("jump");
	}
	
	public void down() {
		System.out.println("go in a hole");
	}
	
	public void left() {
		System.out.println("stop");
	}
	
	public void right() {
		System.out.println("go faster");
	}

}
